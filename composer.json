{
  "name": "marein/php-gaming-website",
  "description": "A gaming website where people can play against each other.",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Reinhold",
      "email": "markusreinhold@icloud.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Gaming\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gaming\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^2.10",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.8",
    "enqueue/amqp-lib": "^0.10.8",
    "jms/serializer": "^3.14",
    "marein/php-nchan-client": "^3.0",
    "marein/symfony-standard-headers-csrf-bundle": "^1.0",
    "predis/predis": "^1.1",
    "psr/log": "^1.1",
    "ramsey/uuid": "^4.1",
    "ramsey/uuid-doctrine": "^1.6",
    "symfony/asset": "^5.2",
    "symfony/console": "^5.2",
    "symfony/dotenv": "^5.2",
    "symfony/finder": "^5.2",
    "symfony/framework-bundle": "^5.2",
    "symfony/monolog-bundle": "^3.6",
    "symfony/twig-bundle": "^5.2",
    "symfony/validator": "^5.2",
    "symfony/yaml": "^5.2",
    "twig/twig": "^3.3"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.3",
    "codeception/module-webdriver": "^1.4",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^0.12.28",
    "phpstan/phpstan-doctrine": "^0.12.44",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.6",
    "symfony/debug-bundle": "^5.1",
    "symfony/error-handler": "^5.1",
    "symfony/stopwatch": "^5.1",
    "symfony/web-profiler-bundle": "^5.1"
  },
  "config": {
    "sort-packages": true
  }
}
